@using Abp.MultiTenancy
@using Lanting.IDCode.Application
@using Lanting.IDCode.Web.Models.Common.Modals
@model GenerateTaskDto
@{
    Layout = null;
}
@Html.Partial("~/Views/Shared/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("EditGenerateTask")))
<div class="modal-body">
    <form name="GenerateTaskEditForm" role="form" novalidate class="form-validation">
        <input type="hidden" name="Id" value="@Model.Id" />
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="UserId" value="@Model.UserId" required maxlength="11">
                <label for="UserId" class="form-label">@L("UserId")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="Remark" value="@Model.Remark">
                <label for="Remark" class="form-label">@L("Remark")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="Created" value="@Model.Created" required maxlength="6">
                <label for="Created" class="form-label">@L("Created")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="">
                <input id="IsSuccess" type="checkbox" name="IsSuccess" value="true" @(Model.IsSuccess ? "checked" : "") class="form-control" />
                <label for="IsSuccess" class="form-label">@L("IsSuccess")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="FailReason" value="@Model.FailReason">
                <label for="FailReason" class="form-label">@L("FailReason")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="Completed" value="@Model.Completed" maxlength="6">
                <label for="Completed" class="form-label">@L("Completed")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="ProductId" value="@Model.ProductId" required maxlength="11">
                <label for="ProductId" class="form-label">@L("ProductId")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="GenerateCount" value="@Model.GenerateCount" required maxlength="11">
                <label for="GenerateCount" class="form-label">@L("GenerateCount")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="TaskStatu" value="@Model.TaskStatu" required maxlength="11">
                <label for="TaskStatu" class="form-label">@L("TaskStatu")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="DataFilePath" value="@Model.DataFilePath">
                <label for="DataFilePath" class="form-label">@L("DataFilePath")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="">
                <input id="IsAntiFake" type="checkbox" name="IsAntiFake" value="true" @(Model.IsAntiFake ? "checked" : "") class="form-control" />
                <label for="IsAntiFake" class="form-label">@L("IsAntiFake")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="AFCodeLength" value="@Model.AFCodeLength" maxlength="11">
                <label for="AFCodeLength" class="form-label">@L("AFCodeLength")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="AntiFackCodeType" value="@Model.AntiFackCodeType" required maxlength="11">
                <label for="AntiFackCodeType" class="form-label">@L("AntiFackCodeType")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="AntiFackCode" value="@Model.AntiFackCode">
                <label for="AntiFackCode" class="form-label">@L("AntiFackCode")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="StartOne" value="@Model.StartOne" required maxlength="20">
                <label for="StartOne" class="form-label">@L("StartOne")</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input class="form-control" type="text" name="EndOne" value="@Model.EndOne" required maxlength="20">
                <label for="EndOne" class="form-label">@L("EndOne")</label>
            </div>
        </div>
    </form>
</div>
@Html.Partial("~/Views/Shared/Modals/_ModalFooterWithSaveAndCancel.cshtml")
<script src="~/view-resources/Views/GenerateTask/_EditGenerateTaskModal.js" asp-append-version="true"></script>